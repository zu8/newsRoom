<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <PreferenceCategory
        app:key="theme_category"
        app:title="Theme switching">
        <SwitchPreferenceCompat
            app:key="theme"
            app:title="Enable dark theme"/>
    </PreferenceCategory>
    <PreferenceCategory
        app:key="country_category"
        app:title="Country choosing">
        <ListPreference
            android:defaultValue="51"
            android:entries="@array/country_entries"
            android:entryValues="@array/country_values"
            android:key="list_preference_countries"
            android:title="@string/title_pref_countries" />
    </PreferenceCategory>


</PreferenceScreen>